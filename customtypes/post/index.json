{
  "id": "post",
  "label": "Artigo de Blog",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "slug",
          "placeholder": ""
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Título",
          "placeholder": ""
        }
      },
      "publishedAt": {
        "type": "Date",
        "config": {
          "label": "Data de publicação",
          "placeholder": ""
        }
      },
      "thumbnail": {
        "type": "Image",
        "config": {
          "label": "Thumbnail",
          "constraint": {},
          "thumbnails": []
        }
      },
      "content": {
        "type": "StructuredText",
        "config": {
          "label": "Conteúdo",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      }
    }
  }
}