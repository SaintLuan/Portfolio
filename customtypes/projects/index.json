{
  "id": "projects",
  "label": "Projects",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "Slug do portfólio",
          "placeholder": ""
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Título",
          "placeholder": ""
        }
      },
      "subtitle": {
        "type": "Text",
        "config": {
          "label": "Subtítulo",
          "placeholder": ""
        }
      },
      "category": {
        "type": "Select",
        "config": {
          "label": "Categoria",
          "placeholder": "",
          "options": [
            "Web Site",
            "Web App",
            "Mobile App",
            "HotSite / Landing Page"
          ]
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Descrição",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "previewUrl": {
        "type": "Link",
        "config": {
          "label": "Link do projeto",
          "placeholder": "",
          "allowTargetBlank": true,
          "select": null
        }
      },
      "publishedAt": {
        "type": "Date",
        "config": {
          "label": "Data da publicação",
          "placeholder": ""
        }
      },
      "thumbnail": {
        "type": "Image",
        "config": {
          "label": "Thumbnail",
          "constraint": {},
          "thumbnails": []
        }
      },
      "features": {
        "type": "StructuredText",
        "config": {
          "label": "Funcionalidades",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "strong,o-list-item,list-item,paragraph,heading3,heading2"
        }
      },
      "images": {
        "type": "Group",
        "config": {
          "label": "Imagens",
          "fields": {
            "screenshot": {
              "type": "Image",
              "config": {
                "label": "Screenshot do projeto",
                "constraint": {},
                "thumbnails": []
              }
            }
          }
        }
      }
    }
  }
}